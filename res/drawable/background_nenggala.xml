<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_vertical|fill_horizontal"
        android:drawable="@drawable/megamendung"/>
    <item
        android:gravity="center_vertical|center_horizontal">
        <rotate android:fromDegrees="45">
            <shape>
                <size
                    android:width="156dp"
                    android:height="156dp" />
                <gradient
                    android:startColor="#fff"
                    android:endColor="#51BFF6" />
                <stroke
                    android:width="5dp"
                    android:color="#fff" />
                <corners android:radius="7dp" />
                <padding
                    android:bottom="15dp"
                    android:right="5dp"
                    />
            </shape>
        </rotate>
    </item>
    <item android:gravity="center_horizontal|center_vertical"
        android:drawable="@drawable/logo_lineage"
        />
</layer-list>